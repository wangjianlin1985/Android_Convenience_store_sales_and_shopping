<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.chengxusheji.domain">

	<class name="Seller" table="Seller">

          <!-- 主键商家账号 -->
          <id name="sellUserName" type="string" length="20">
            <generator class="assigned"/>
          </id>

          <!-- 简单字符串属性: 登录密码 -->
          <property
            name="password"
            column="password"
            type="string"
            length="30"
          />

          <!-- 简单字符串属性: 商家名称 -->
          <property
            name="sellerName"
            column="sellerName"
            type="string"
            length="20"
          />

          <!-- 简单字符串属性: 联系电话 -->
          <property
            name="telephone"
            column="telephone"
            type="string"
            length="20"
          />

          <!-- 简单字符串属性: 商家地址 -->
          <property
            name="address"
            column="address"
            type="string"
            length="80"
          />

          <!-- 简单字符串属性: 入住时间 -->
          <property
            name="regTime"
            column="regTime"
            type="string"
            length="30"
          />


		 
		
	</class>
	
</hibernate-mapping>
 
